{
    "name": "@gamingisaac/rng",
    "version": "1.0.2-alpha",
    "description": "GamingIsaac Random Number Generator Module",
    "main": "index.js",
    "scripts": {
        "prepare-test": "ts-node src/test2.ts",
        "diehard-test": "npm run prepare-test && dieharder -g 202 -f testrng.txt -d0",
        "test": "ts-node scripts/RNG-Test.ts",
        "start": "node $NODE_DEBUG_OPTION node_modules/.bin/ts-node src",
        "build": "tsc && mv src/*.js src/*.d.ts src/*.js.map .",
        "rerun-checksum": "rm src.sha1 && find ./src -type f -print0 | xargs -0 -n1 shasum {} > src.sha1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://bitbucket.org/clientframeworkgroup/rng.git"
    },
    "author": "",
    "license": "UNLICENSED",
    "dependencies": {
        "@types/node": "^10.11.0",
        "cli-progress": "^2.1.1",
        "numeral": "^2.0.6",
        "shelljs": "^0.8.3",
        "ts-node": "^7.0.1",
        "typescript": "^3.0.3"
    },
    "devDependencies": {
        "numeraljs": "^1.5.6",
        "readline": "^1.3.0"
    },
    "homepage": "https://bitbucket.org/clientframeworkgroup/rng#readme"
}
