        /* https://clientframework-studios.atlassian.net/browse/FLSP-188
        Pinch zooming and dragging on iphone*/
        let doubleTouchStartTimestamp = 0;
        document.addEventListener("touchstart", function(evt){
                evt.stopPropagation();
        let now = +(new Date());
        if (doubleTouchStartTimestamp + 500 > now){
                event.preventDefault();
        };
        doubleTouchStartTimestamp = now;
        if(evt.touches.length > 1){
                evt.preventDefault();
        }
        },{passive: false});

        let resiz = function() {
                const _GAME_WIDTH_DEFAULT = 1920;
                const _GAME_HEIGHT_DEFAULT = 1080;
                let scaleX = 0;
                let scaleY = 0;
                let isPort = (document.documentElement.clientHeight > document.documentElement.clientWidth)
                // temporary fix for pixelisation issue. old system only left on iphone
                if (navigator.userAgent.indexOf("iPhone") != -1) {
                        // if (isPort) {
                        // scaleX = Math.min((document.documentElement.clientHeight) / _GAME_WIDTH_DEFAULT, (document.documentElement.clientWidth) / _GAME_HEIGHT_DEFAULT)
                        // scaleY = Math.min((document.documentElement.clientHeight) / _GAME_WIDTH_DEFAULT, (document.documentElement.clientWidth) / _GAME_HEIGHT_DEFAULT)
                        // }
                        // else
                        {
                        scaleX = Math.min((document.documentElement.clientHeight) / _GAME_HEIGHT_DEFAULT, (document.documentElement.clientWidth) / _GAME_WIDTH_DEFAULT)
                        scaleY = Math.min((document.documentElement.clientHeight) / _GAME_HEIGHT_DEFAULT, (document.documentElement.clientWidth) / _GAME_WIDTH_DEFAULT)
                        }
                        if (isPort) {
                                scaleX *= 1.3;
                                scaleY *= 1.2;
                        }
                }
                else {
                        if (isPort) {
                                scaleX = Math.min((document.documentElement.clientHeight) / _GAME_WIDTH_DEFAULT, (document.documentElement.clientWidth) / _GAME_HEIGHT_DEFAULT)
                                scaleY = Math.min((document.documentElement.clientHeight) / _GAME_WIDTH_DEFAULT, (document.documentElement.clientWidth) / _GAME_HEIGHT_DEFAULT)
                        }
                        else {
                                scaleX = Math.min((document.documentElement.clientHeight) / _GAME_HEIGHT_DEFAULT, (document.documentElement.clientWidth) / _GAME_WIDTH_DEFAULT)
                                scaleY = Math.min((document.documentElement.clientHeight) / _GAME_HEIGHT_DEFAULT, (document.documentElement.clientWidth) / _GAME_WIDTH_DEFAULT)
                        }
                        if (isPort) {
                                scaleX *= 0.7;
                                scaleY *= 0.7;
                        }
                }
                let  infoPageHTML = document.getElementById("infoPage")
                infoPageHTML.style.transform = infoPageHTML.style.webkitTransform = `translate(-50%, -${navigator.userAgent.indexOf("iPhone") != 1?52:50}%) scale(${scaleX}, ${scaleY})`;

                let  rulesPageHTML = document.getElementById("rulesPage")
                if (navigator.userAgent.indexOf("Mobile") == -1) {
                        rulesPageHTML.style.transform = rulesPageHTML.style.webkitTransform = `translate(-50%, -${navigator.userAgent.indexOf("iPhone") != 1?52:50}%) scale(${0.9 * scaleX}, ${0.9 * scaleY})`;
                }
                else {
                        rulesPageHTML.style.transform = rulesPageHTML.style.webkitTransform = `translate(-50%, -${navigator.userAgent.indexOf("iPhone") != 1?52:50}%) scale(${scaleX}, ${scaleY})`;
                }
        }

        let gup = function(name) {
                const url = window.location.href;
                name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
                var regexS = "[\\?&]"+name+"=([^&#]*)";
                var regex = new RegExp( regexS );
                var results = regex.exec( url );
                return results == null ? null : results[1];
        }

        let trimLanguages = function() {
                let z, i, elmnt;

                let supportedlanguages = ["en", "de", "fr", "it", "sv","bg","cs","da","el","hu","ja","es","es-cl","fi","no","pl","pt","ro","ru","sk","th","zhHant","zhHans","vi","tr"];

                let parameter = gup("language");
                let lang = (parameter && supportedlanguages.indexOf(parameter) != -1) ? parameter : "en";
                //console.log("laaang 1", lang)

                z = document.getElementsByTagName("*");
                for (i = 0; i < z.length; i++) {
                        elmnt = z[i];
                        if (elmnt.hasAttribute("languageSensitive") && elmnt.getAttribute("languageSensitive") != lang) {
                                elmnt.parentNode.removeChild(elmnt);
                                //console.log("laaang 2", elmnt)
                                trimLanguages();
                                return;
                        }
                }
        }

        window.onload = function() {
                trimLanguages();
                resiz();
        }

        window.onresize = resiz;

        </script>
